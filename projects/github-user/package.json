{
  "name": "github-user",
  "version": "1.0.1",
  "private": true,
  "description": "A solution for \"Github user search app\" challenge on frontendmentor.io",
  "license": "MIT",
  "repository": "git@github.com:alex-kim-dev/frontend-practice.git",
  "author": "Alex Kim (https://github.com/alex-kim-dev)",
  "homepage": "https://alex-kim-dev.github.io/frontend-practice/",
  "bugs": "https://github.com/alex-kim-dev/frontend-practice/issues",
  "scripts": {
    "build": "vite build",
    "fix": "concurrently \"npm:fix:*\"",
    "fix:code": "npm run lint:code -- --fix",
    "fix:format": "npm run lint:format -- --write",
    "fix:styles": "npm run lint:styles -- --fix",
    "lint": "concurrently \"npm:lint:*\"",
    "lint:code": "eslint --ignore-path .gitignore .",
    "lint:format": "prettier --ignore-path .gitignore --check \"**/*.{json,yml,md}\"",
    "lint:styles": "stylelint --ignore-path .gitignore \"**/*.css\"",
    "precommit": "lint-staged --quiet",
    "serve": "vite preview",
    "start": "vite"
  },
  "dependencies": {
    "@frontend/site-meta": "^1.0.2"
  },
  "devDependencies": {
    "concurrently": "^6.5.1",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^12.1.4",
    "modern-css-reset": "^1.4.0",
    "prettier": "^2.5.1",
    "prettier-plugin-package": "^1.3.0",
    "stylelint": "^14.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "vite": "^2.7.8",
    "vite-plugin-html": "^2.1.2"
  }
}
